            <xml-property name="queryText"><![CDATA[SELECT 
    l.titulo,
    l.autor,
    COUNT(p.id_libro_prestamo) AS cantidad_prestamos
FROM 
    libro l
JOIN 
    prestamo p
ON 
    l.id_libro = p.id_libro_prestamo
GROUP BY 
    l.id_libro, l.titulo, l.autor
ORDER BY 
    cantidad_prestamos DESC;
]]></xml-property>
